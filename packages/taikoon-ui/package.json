{
  "name": "taikoon-ui",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "cd ../ui-lib && pnpm build && cd - && vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check": "npm run svelte:check",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check ./src && eslint ./src --fix",
    "format": "prettier --write ./src",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "start": "npm run generate:abi && vite dev --open --host 0.0.0.0",
    "start:prod": "vite dev --open --host 0.0.0.0 --mode production",
    "generate:abi": "cd ../nfts && pnpm compile && cd - && wagmi generate"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@playwright/test": "^1.49.1",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.15.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/eslint": "^8.56.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "convert-csv-to-json": "^2.50.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-svelte": "^2.46.1",
    "p-map": "^7.0.3",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "raw-body": "^2.5.2",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-copy": "^1.4.2",
    "svelte-i18n": "^4.0.1",
    "svelte-scrolling": "^1.5.1",
    "tailwindcss-image-rendering": "^1.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "which": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@openzeppelin/merkle-tree": "^1.0.7",
    "@taiko/ui-lib": "workspace:*",
    "@wagmi/cli": "^2.1.22",
    "@wagmi/connectors": "^4.3.10",
    "@wagmi/core": "^2.16.3",
    "@web3modal/common": "^5.1.11",
    "@web3modal/ui": "^4.2.3",
    "@web3modal/wagmi": "^4.2.3",
    "@zerodevx/svelte-toast": "^0.9.6",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "dayjs": "^1.11.13",
    "debug": "^4.4.0",
    "minidenticons": "^4.2.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "viem": "^2.21.57"
  }
}
